#include <bits/stdc++.h>
using namespace std;

int main() {
	int n;
	cin >> n;

	cout << n << " ";

	vector<int> process;

	while (n != 1) {
	if (n % 2 ==0) { n = n / 2; process.push_back(n); } 
	else { n = (n * 3) + 1; process.push_back(n); }
	}

	for (int i = 0; i < process.size(); i++) {
		cout << process[i] << " ";
	}

}
